{
  "name": "praxigento/mobi_app_generic_mage2_work",
  "description": "Development/test environment for Magento v2 Generic MOBI application.",
  "homepage": "https://github.com/praxigento/mobi_app_generic_mage2",
  "authors": [
    {
      "name": "Alex Gusev",
      "email": "flancer64@gmail.com"
    }
  ],
  "require": {
    "praxigento/composer_plugin_templates": "1.0.6",
    "praxigento/mobi_app_generic_mage2": "dev-master"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^2",
    "mockery/mockery": "^0.9.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Praxigento\\Test\\": "../dev/fw/"
    }
  },
  "repositories": {
    "praxigento/mobi_app_generic_mage2": {
      "type": "vcs",
      "url": "https://github.com/praxigento/mobi_app_generic_mage2"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "praxigento_templates_config": [
      "../deploy/work/templates.cfg.json",
      "../templates.vars.work.json"
    ]
  }
}